<idea-plugin>
    <id>com.github.braisdom.object-sql-intellij</id>
    <name>ObjectSQL</name>
    <vendor email="wangyonghe@msn.com" url="http://www.objectsql.com">Braisdom</vendor>
    <version>1.0</version>
    <description><![CDATA[Filling the generated elements for ObjectSql]]></description>
    <change-notes><![CDATA[ The first release for ObjectiveSQL ]]></change-notes>

    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <lang.psiAugmentProvider implementation="com.github.braisdom.objsql.intellij.ObjSqlPsiAugmentProvider"/>
        <implicitUsageProvider implementation="com.github.braisdom.objsql.intellij.ObjSqlImplicitUsageProvider"/>
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>